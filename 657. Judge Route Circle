class Solution {
    public boolean judgeCircle(String moves) {
        int u=count(moves,'U');
        int d=count(moves,'D');
        int l=count(moves,'L');
        int r=count(moves,'R');
        if(u==d&l==r){
            return true;
        }
        return false;
    }
    public int count(String s, char a){
        int count=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)==a){
                count++;
            }
        }
        return count;
    }
}
